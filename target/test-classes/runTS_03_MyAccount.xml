<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="3">
	<listeners>
		<listener class-name ="reportConfigV3.ExtentTestListener"></listener>
	</listeners>
	
	<test name="Test case run on Firefox browser">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.nopcommerce.demo.TS_03_MyAccount" />
		</classes>
	</test>
	
 	<test name="Test case run on Chrome browser">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.nopcommerce.demo.TS_03_MyAccount" />
		</classes>

	</test>
	
	<test name="Test case run on Edge browser">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.nopcommerce.demo.TS_03_MyAccount" />
		</classes>
	</test> 


</suite> <!-- Suite -->
